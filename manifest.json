{
  "manifest_version": 3,
  "name": "Google Meet Summarizer",
  "version": "1.0",
  "description": "Summarizes Google Meet conversations in listicle format",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["https://meet.google.com/*"],
    "js": ["content.js"]
  }]
}